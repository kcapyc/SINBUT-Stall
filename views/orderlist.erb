<p><strong class="cap"><%=session[:identity]%></strong>, для вас подготовлен список заказов:</p>

  <table class="table table-striped">
    <thead>
      <tr>
        <th>Имя</th>
        <th>Телефон</th>
        <th>E-mail</th>
        <th>Город</th>
        <th>Заказ</th>
        <th>Дата</th>
      </tr>
    </thead>
    <tbody>
    <% @order.each do |client| %>
      <tr>
        <td><%= client.name %></td>
        <td><%= client.phone %></td>
        <td><%= client.email %></td>
        <td><%= client.address %></td>
        <td><%= client.order_input %></td>
        <td><%= client.created_at %></td>
      </tr>
      <% end %>
    </tbody>
  </table>